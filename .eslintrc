{
  "extends": "airbnb",
  "rules": {
    "no-param-reassign": "warn",
    "no-underscore-dangle": "warn",
    "max-len": [1, 120, 2, {"ignoreComments": true}],
    "camelcase": [0],
    "no-cond-assign": [2, "except-parens"],
    "no-else-return": 0,
    "quotes": 0,
    "consistent-return": 0,
    "no-trailing-spaces": 0,
    "operator-assignment": 0,
    "no-console": 0,
    "class-methods-use-this": 0,
    "new-cap": 0,
    "no-path-concat": 0,
    "prefer-template": 0
  }
}